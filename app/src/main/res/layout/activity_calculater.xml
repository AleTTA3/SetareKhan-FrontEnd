<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="434dp"
        android:layout_height="0dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="31dp"
        android:layout_height="952dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="447dp"
        android:layout_height="422dp"
        android:layout_marginStart="84dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/frameLayout"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="104dp">

        </FrameLayout>

        <EditText
            android:id="@+id/txt_sum"
            android:layout_width="match_parent"
            android:layout_height="96dp"
            android:ems="10"
            android:inputType="numberDecimal" />

        <EditText
            android:id="@+id/txt_result"
            android:layout_width="match_parent"
            android:layout_height="84dp"
            android:ems="10"
            android:gravity="left"
            android:inputType="numberDecimal" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="435dp"
        android:layout_height="415dp"
        android:layout_marginStart="76dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/frameLayout"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="448dp">

            <TableRow
                android:layout_width="147dp"
                android:layout_height="match_parent"
                android:layout_marginRight="80dip"
                android:gravity="right"/>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <Button
                    android:id="@+id/btn_seven"
                    android:layout_width="82dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="7"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_eight"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="8"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_nine"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="9"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_menha"
                    android:layout_width="112dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/btn"
                    android:text="-"
                    android:textSize="40sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <Button
                    android:id="@+id/btn_for"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="4"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_five"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="5"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_six"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="6"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_jam"
                    android:layout_width="112dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/btn"
                    android:text="+"
                    android:textSize="40sp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <Button
                    android:id="@+id/btn_one"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="1"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_two"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="2"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_tree"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="3"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_zarb"
                    android:layout_width="112dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/btn"
                    android:text="ร"
                    android:textSize="40sp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <Button
                    android:id="@+id/btn_ac"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btn"
                    android:text="AC"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_zero"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btngray"
                    android:text="0"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_taghsim"
                    android:layout_width="78dp"
                    android:layout_height="76dp"
                    android:background="@drawable/btn"
                    android:text="รท"
                    android:textSize="40sp" />

                <Button
                    android:id="@+id/btn_sum"
                    android:layout_width="112dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/btnorange"
                    android:text="="
                    android:textSize="40sp" />
            </TableRow>
        </TableLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>